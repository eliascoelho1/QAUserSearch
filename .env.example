# QAUserSearch - Environment Variables Template
# Copy this file to .env and fill in the values

# Application
APP_NAME=qausersearch
ENVIRONMENT=development
DEBUG=false
LOG_LEVEL=INFO

# Database - Application (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/qausersearch

# Database - QA (read-only access to existing QA database)
QA_DATABASE_URL=postgresql+asyncpg://qa_reader:password@qa-db-host:5432/qa_database

# Server
HOST=0.0.0.0
PORT=8000

# CORS
ALLOWED_HOSTS=["*"]
CORS_ORIGINS=["http://localhost:3000"]

# Metrics
METRICS_ENABLED=true
